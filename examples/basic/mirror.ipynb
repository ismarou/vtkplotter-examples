    "\"\"\"Mirror a mesh along one of the Cartesian axes.\"\"\"\n",
    "\n",
    "from vtkplotter import *\n",
    "\n",
    "myted1 = load(datadir+\"teddy.vtk\")\n",
    "\n",
    "myted2 = myted1.clone().mirror(\"y\").pos(0,3,0).color(\"green\")\n",
    "\n",
    "show(myted1, myted2, viewup='z')"
